<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.mychurch" name="Church Online" version="3.0" provider-name="">
    <requires>
        <import addon="xbmc.python" version="2.25.0"/>
        <import addon="script.module.routing" version="0.2.0"/>
        <import addon="script.module.requests" version="2.12.4"/>
        <import addon="plugin.video.youtube"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="news.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en_GB">Church Online</summary>
        <description lang="en_GB">We are a non denomination church that has a passion for interacting with people all over the world. Every Sunday we broadcast live and give you the ability to chat with a pastor and submit prayer request. Come join us and experience church like never before.</description>
        <language>en pt</language>
        <platform>all</platform>
        <license>GPL-3.0</license>
        <news></news>
        <disclaimer>The author of this add-on is in no way affiliated with Church Online</disclaimer>
        <assets>
        <icon>resources/images/icon.png</icon>
        <fanart>resources/images/fanart.jpg</fanart>
        </assets>
    </extension>
</addon>
