<addon id="plugin.video.NZtv3vod"
       name="NZ TV3 VOD Content"
       version="1.0.6"
       provider-name="@Kiwi_man82 and @teamxkodi - TeamX">
  <requires>
    <import addon="xbmc.python" version="2.24.0"/>
    <import addon="script.module.requests"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Unoffical New Zealand TV3 VOD addon</summary>
    <language>en</language>
    <description lang="en">This addon scrapes the New Zealand TV3 website for the VOD content and brings it to you on kodi - TeamX do not host any of the content found in this addon, all we did was bring this content to you from the internet.  Shout outs to NoobsAndNerds for pointing me in the right direction (whufclee) and Pipcan for the base code of this addon along with his regex tutorial on Youtube</description>
     <platform>all</platform>
  </extension>
</addon>

