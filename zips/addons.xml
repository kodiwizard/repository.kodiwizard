<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.program.kodiwizard" name="Team Wizard" version="1.0" provider-name="Team Wizard">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Team Wizard</summary>
    <description lang="en">Team Wizard
[CR]
[CR]Get Team Wizard Builds</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.kodiwizard" name="Kodi Wizard" version="0.0.1" provider-name="Kodi Wizard">
	<extension point="xbmc.addon.repository" name="Kodi Wizard">
		<dir>
			<info compressed="false">https://raw.githubusercontent.com/totalrevolution/python-koding/master/zips/addons.xml</info>
			<checksum>https://raw.githubusercontent.com/totalrevolution/python-koding/master/zips/addons.xml.md5</checksum>
			<datadir zip="true">https://raw.githubusercontent.com/totalrevolution/python-koding/master/zips/</datadir>
		</dir>
		<info compressed="false">https://raw.githubusercontent.com/kodiwizard/repository.kodiwizard/master/zips/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/kodiwizard/repository.kodiwizard/master/zips/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/kodiwizard/repository.kodiwizard/master/zips/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Kodi Wizard Builds</summary>
