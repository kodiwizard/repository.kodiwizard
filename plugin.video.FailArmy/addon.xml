<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.FailArmy" name="FailArmy" version="2.2" provider-name="">
    <requires>
        <import addon="xbmc.python" version="2.25.0"/>
        <import addon="script.module.routing" version="0.2.0"/>
        <import addon="script.module.requests" version="2.12.4"/>
        <import addon="plugin.video.youtube"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="failarmy.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en_GB">FailArmy Youtube</summary>
        <description lang="en_GB">FailArmy is the world’s number one source for epic fail videos and hilarious compilations. We’re powered by fan submissions and feedback from all around the world, with over 30 million fans across digital platforms.
		</description>
        <language>en pt</language>
        <platform>all</platform>
        <license>GPL-3.0</license>
        <news></news>
        <disclaimer>The author of this add-on is in no way affiliated with FailArmy</disclaimer>
        <assets>
        <icon>resources/images/icon.png</icon>
        <fanart>resources/images/fanart.jpg</fanart>
        </assets>
    </extension>
</addon>
