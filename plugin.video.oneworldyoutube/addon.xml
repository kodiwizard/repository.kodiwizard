<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.oneworldyoutube" name="One World Youtube" version="1.2" provider-name="Team X Kodi">
    <requires>
        <import addon="xbmc.python" version="2.25.0"/>
        <import addon="script.module.routing" version="0.2.0"/>
        <import addon="script.module.requests" version="2.12.4"/>
        <import addon="plugin.video.youtube"/>
        <import addon="repository.oneworlddigital"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="alpha.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en_GB">One World Digital Solutions Youtube</summary>
        <description lang="en_GB">One World Digital Solutions "Welcome To The Future Of Home Entertainment" This channel is to show our One World Digital Solutions Custom HD Streaming/Retro gaming devices. </description>
        <language>en pt</language>
        <platform>all</platform>
        <license>GPL-3.0</license>
        <news></news>
        <disclaimer></disclaimer>
        <assets>
        <icon>resources/images/icon.png</icon>
        <fanart>resources/images/fanart.jpg</fanart>
        </assets>
    </extension>
</addon>
